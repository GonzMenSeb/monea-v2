{
  "name": "monea",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "postinstall": "patch-package",
    "start": "expo start",
    "android": "expo run:android",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm test",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:build:release": "detox build --configuration android.emu.release",
    "e2e:test:release": "detox test --configuration android.emu.release",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@lovesworking/watermelondb-expo-plugin-sdk-52-plus": "^1.0.3",
    "@maniac-tech/react-native-expo-read-sms": "^9.0.2-alpha",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@shopify/flash-list": "^2.0.2",
    "@shopify/react-native-skia": "^2.4.14",
    "@tanstack/react-query": "^5.90.18",
    "expo": "~54.0.31",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.13",
    "expo-haptics": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-network": "~8.0.8",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-vector-icons": "^10.3.0",
    "victory-native": "^41.20.2",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@config-plugins/detox": "^11.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.9",
    "detox": "^20.46.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "^54.0.16",
    "patch-package": "^8.0.1",
    "prettier": "^3.8.0",
    "react-native-worklets": "^0.7.2",
